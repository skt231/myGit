<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.drug.dao.IUsersDao">
<insert id="insert">
insert into users(id,pw,name,email,address,phone,useyn,regdate) values(#{id},#{pw},#{name},#{email},#{address},#{phone},#{useyn},#{regdate})
</insert>
<select id="selectAll" resultType="UsersDto">
select * from users
</select>
<select id="selectId" resultType="UsersDto">
select * from users where id=#{id}
</select>
<update id="update">
update users set pw=#{pw},email=#{email},address=#{address},phone=#{phone} where id=#{id}
</update>
<delete id="delete">
delete users where id=#{id}
</delete>
</mapper>