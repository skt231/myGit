<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.dao.ItestDao">
<insert id="insert">
insert into test_1 values (#{name},#{sex},#{phone},#{registeration},#{statement})
</insert>
<select id="selectAll" resultType="testDto">
select * from test_1
</select>
<select id="selectName" resultType="testDto">
select * from test_1 where name=#{name}
</select>
<update id="update">
update test_1 set phone=#{phone},#{statement} where name=#{name}
</update>
<delete id="delete">
delete test_1 where name=#{name}
</delete>
</mapper>